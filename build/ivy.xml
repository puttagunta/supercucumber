    <artifact name="cucumber-extensions" type="jar" ext="jar" conf="archives,runtime"/>
  </publications>
  <dependencies>
    <dependency org="org.codehaus.groovy" name="groovy-all" rev="2.3.11" conf="compile-&gt;default"/>
